
<% if (entries.length === 0) { %>
    <p><i>No entries</i></p>
<% } else { %>
    <ul role="list" class="list-of-links">
        <% for (const post of it.entries) { %>
            <% if (post.data.alt?.[lang]) continue %>
            <li><%~ comp.miniEntry({ post, dates, lang: it.lang }) %></li>
        <% } %>
    </ul>
<% } %>
