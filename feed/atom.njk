---
layout: false
url: '/feed.xml'
---
<?xml version="1.0" encoding="utf-8"?>

<feed xmlns="http://www.w3.org/2005/Atom">
  <title>Deniz Akşimşek</title>
  <link href="{{ url | url(true) }}" rel="self"/>
  <link href="{{ '/' | url(true )}}"/>
  <updated>{{ (search.pages() | last).data.date | date("ATOM") }}</updated>
  <id>{{ '/' | url(true )}}</id>
  <author>
    <name>Deniz Akşimşek</name>
    <email>deniz@denizaksimsek.com</email>
  </author>
  {%- for post in search.pages("page.src.path^=/entries", "date=desc") %}
  <entry>
    <title>{{ post.data.title }}</title>
    <link href="{{ post.data.url | url(true) }}"/>
    <updated>{{ post.data.date | date("ATOM") }}</updated>
    <id>{{ post.data.url | url(true) }}</id>
    <content type="html">{{ post.data.content | md | htmlUrl(true) }}</content>
  </entry>
  {%- endfor %}
</feed>
